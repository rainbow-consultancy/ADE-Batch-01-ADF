								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"variableName": "fileExtension",
								"value": {
									"value": "@last(split(item().name, '.'))",
									"type": "Expression"
								}
							}
						},
						{
							"name": "SwitchBasedOnFileType",
							"type": "Switch",
							"dependsOn": [
								{
									"activity": "SetFileExtension",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"userProperties": [],
							"typeProperties": {
								"on": {
									"value": "@variables('fileExtension')",
									"type": "Expression"
								},
								"cases": [
									{
										"value": "csv",
										"activities": [
											{
												"name": "CopyCsvFiles",
												"type": "Copy",
												"dependsOn": [],
												"policy": {
													"timeout": "0.12:00:00",
													"retry": 0,
													"retryIntervalInSeconds": 30,
													"secureOutput": false,
													"secureInput": false
												},
												"userProperties": [],
												"typeProperties": {
													"source": {
														"type": "BinarySource",
														"storeSettings": {
															"type": "AzureBlobFSReadSettings",
															"recursive": true,
															"deleteFilesAfterCompletion": false
														},
														"formatSettings": {
															"type": "BinaryReadSettings"
														}
													},
													"sink": {
														"type": "BinarySink",
														"storeSettings": {
															"type": "AzureBlobFSWriteSettings"
														}
													},
													"enableStaging": false
												},
												"inputs": [
													{
														"referenceName": "DS_TARGET_ADLS_BINARY",
														"type": "DatasetReference",
														"parameters": {
															"TargetFolder": "raw-datasets",
															"TargetFile": {
																"value": "@item().name",
																"type": "Expression"
															}
														}
													}
												],
												"outputs": [
													{
														"referenceName": "DS_TARGET_ADLS_BINARY",
														"type": "DatasetReference",
														"parameters": {
															"TargetFolder": {
																"value": "@toUpper(variables('fileExtension'))",
																"type": "Expression"
															},
															"TargetFile": {
																"value": "@item().name",
																"type": "Expression"
															}
														}
													}
												]
											}
										]
									},
									{
										"value": "json",

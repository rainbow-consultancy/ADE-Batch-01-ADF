														"parameters": {
															"TargetFolder": "raw-datasets",
															"TargetFile": {
																"value": "@item().name",
																"type": "Expression"
															}
														}
													}
												],
												"outputs": [
													{
														"referenceName": "DS_TARGET_ADLS_BINARY",
														"type": "DatasetReference",
														"parameters": {
															"TargetFolder": {
																"value": "@toUpper(variables('fileExtension'))",
																"type": "Expression"
															},
															"TargetFile": {
																"value": "@item().name",
																"type": "Expression"
															}
														}
													}
												]
											}
										]
									},
									{
										"value": "parquet",
